{
  "expo": {
    "name": "DAX - Income Tracker",
    "slug": "dax-income-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/accounting.png",
    "scheme": "dax",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.dax.incometracker"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/accounting.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.dax.incometracker"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/accounting.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-file-system"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}